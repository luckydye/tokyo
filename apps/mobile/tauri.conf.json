{
  "build": {
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "desktop",
    "version": "0.0.0"
  },
  "tauri": {
    "bundle": {
      "iOS": {
        "developmentTeam": "luckydye"
      },
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "transparent": true,
        "fileDropEnabled": true,
        "decorations": false,
        "hiddenTitle": true,
        "fullscreen": false,
        "resizable": true,
        "title": "TOKYO",
        "width": 1400,
        "height": 900
      }
    ],
    "macOSPrivateApi": true
  }
}
